<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.rtl.min.css" integrity="sha384-OXTEbYDqaX2ZY/BOaZV/yFGChYHtrXH2nyXJ372n2Y8abBhrqacCEe+3qhSHtLjy" crossorigin="anonymous">
    <script src="https://unpkg.com/boxicons@2.1.2/dist/boxicons.js"></script>
    <title>NUEVO NACIMIENTO</title>
    <style>
      body{
        background-color: rgb(177, 195, 214);
      }
    </style>
  </head>
  <body>
    <center style="padding-top: 10px;"><h1>NUEVO NACIMIENTO</h1> </center>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous"></script>
      <div class="container mt-4">
        <div class="card border-secondary" style="background-color: rgba(154, 149, 182, 0.578);">
            <div class="card-header">Nuevo Registro</div>
            <div class="card-body">
                <form action="/save5" method="POST">
                
                <div class="">
                   <!-- <label for="user" class="form-label">ARETE DE LA CERDA</label>   -->
                  
                  <input type="text" class="form-control" id="aretecerda" name="aretecerda" value="<%=user.arete %>" hidden>
              </div>
               
                <div class="mb-3">
                  <!--  <label for="user" class="form-label">NOMBRE DE LA CERDA</label>   -->
                 
                  <input type="text" class="form-control" id="cerda" name="cerda" value="<%=user.nombre %>" hidden>
              </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">CODIGO DE PARTO</label>
                        <input type="text" class="form-control" id="lotelechon" name="lotelechon" tabindex="1">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">PRECIO</label>
                        <input type="number" class="form-control" id="precio" name="precio" tabindex="1">
                      </div>
                      <div class="mb-3">
                        <!--    -->
                        <!--<label for="user" class="form-label">CANTIDAD DISPONIBLE PARA LA VENTA</label>-->
                        <input type="number" class="form-control" id="cantidad" name="cantidad" value="nacvivos" hidden>
                      </div>
                      <div class="mb-3">
                        <label for="user" class="form-label">PARTO NO.</label>
                        <input type="number" class="form-control" id="nparto" name="nparto" tabindex="1">
                      </div>
                      <div class="mb-3">
                        <label for="user" class="form-label">NACIDOS VIVOS</label>
                        <input type="text" class="form-control" id="nacvivos" name="nacvivos" tabindex="1" step="0.001" oninput="calcular()">
                      </div>
                      <div class="mb-3">
                        <label for="user" class="form-label">NACIDOS MUERTOS</label>
                        <input type="text" class="form-control" id="nacmuertos" name="nacmuertos" tabindex="1" step="0.001" oninput="calcular()">
                      </div>
                      <div class="mb-3">
                        <label for="user" class="form-label">MOMIAS</label>
                        <input type="text" class="form-control" id="momias" name="momias" tabindex="1" step="0.001" oninput="calcular()">
                      </div>
                      <div class="mb-3">
                          <!--  <label for="user" class="form-label">TOTAL DE NACIDOS</label>  -->
                        
                        <input type="text" class="form-control" id="nactotal" name="nactotal" hidden>
                      </div>
                      <div class="mb-3">
                        <label for="user" class="form-label">FECHA DE PARTO</label>
                        <input type="date" class="form-control" id="fechaparto" name="fechaparto" tabindex="1">
                      </div>
                      <div class="mb-3">
                        <label for="user" class="form-label">ATENDIDO POR</label>
                        <input type="text" class="form-control" id="atendido" name="atendido" tabindex="1">
                      </div>
                      <SCript type="text/javascript">
                        function calcular(){
                          try{
                            var a= parseFloat(document.getElementById("nacvivos").value)||0,
                                b= parseFloat(document.getElementById("nacmuertos").value)||0;
                                c= parseFloat(document.getElementById("momias").value)||0;
                            
                                document.getElementById("nactotal").value=a+b+c;
                                document.getElementById("cantidad").value=a;

                          }catch(e){}
                        }
                      </SCript>
                      

                    <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                    <a href="/menu_ingreso_cerda" class="btn btn-secondary">Cancelar</a>
                </form>
            </div>
        </div>
      </div>
   
  </body>
</html>